<h2 style="color:crimson;">Historial</h2>

{{#if productos}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Precio</th> <th>URL</th> </tr>
            {{#each productos}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{precio}}</td>
                <td>{{url}}</td>
            </tr>
            {{/each}}
        </table>
    </div>

{{else}}
    <h3 class="alert alert-danger">no se encontraron datos</h3>
{{/if}}